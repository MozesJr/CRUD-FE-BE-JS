nt-crud/frontend/src/components/StudentList.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <div>\r\n    <h1>Student List</h1>\r\n    <button class=\"btn btn-primary mb-3\" @click=\"addStudent\">Add Student</button>\r\n    <table class=\"table\">\r\n      <thead>\r\n        <tr>\r\n          <th>Name</th>\r\n          <th>Student ID</th>\r\n          <th>Major</th>\r\n          <th>Gender</th>\r\n          <th>Actions</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"student in students\" :key=\"student._id\">\r\n          <td>{{ student.name }}</td>\r\n          <td>{{ student.studentId }}</td>\r\n          <td>{{ student.major }}</td>\r\n          <td>{{ student.gender }}</td>\r\n          <td>\r\n            <button class=\"btn btn-warning\" @click=\"editStudent(student)\">Edit</button>\r\n            <button class=\"btn btn-danger\" @click=\"confirmDeleteStudent(student._id)\">Delete</button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport Swal from 'sweetalert2';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      students: [],\r\n    };\r\n  },\r\n  methods: {\r\n    async fetchStudents() {\r\n      const response = await axios.get('http://localhost:5000/api/students');\r\n      this.students = response.data;\r\n    },\r\n    addStudent() {\r\n      this.$emit('edit-student', null);\r\n    },\r\n    editStudent(student) {\r\n      this.$emit('edit-student', student);\r\n    },\r\n    confirmDeleteStudent(id) {\r\n      Swal.fire({\r\n        title: 'Are you sure?',\r\n        text: 'You won\\'t be able to revert this!',\r\n        icon: 'warning',\r\n        showCancelButton: true,\r\n        confirmButtonColor: '#3085d6',\r\n        cancelButtonColor: '#d33',\r\n        confirmButtonText: 'Yes, delete it!',\r\n      }).then(async (result) => {\r\n        if (result.isConfirmed) {\r\n          await axios.delete(`http://localhost:5000/api/students/${id}`);\r\n          this.fetchStudents();\r\n          Swal.fire('Deleted!', 'Student has been deleted.', 'success');\r\n        }\r\n      });\r\n    },\r\n  },\r\n  created() {\r\n    this.fetchStudents();\r\n  },\r\n};\r\n</script>\r\n"]}]}